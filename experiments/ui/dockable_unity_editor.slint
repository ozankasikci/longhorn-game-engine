export component DockableUnityEditor inherits Window {
    title: "Unity Editor - Dockable Panels (Slint)";
    width: 1400px;
    height: 900px;
    background: #1e1e1e;
    
    // Properties
    in property <[GameObject]> scene-objects: [];
    in property <int> selected-object-index: -1;
    in property <[ConsoleMessage]> console-messages: [];
    
    // Panel states
    in property <bool> hierarchy-floating: false;
    in property <bool> inspector-floating: false;
    in property <bool> console-floating: false;
    in property <bool> project-floating: false;
    
    in property <float> hierarchy-x: 100;
    in property <float> hierarchy-y: 100;
    in property <float> inspector-x: 200;
    in property <float> inspector-y: 150;
    in property <float> console-x: 300;
    in property <float> console-y: 200;
    in property <float> project-x: 400;
    in property <float> project-y: 250;
    
    // Callbacks
    callback object-selected(int);
    callback object-created();
    callback object-deleted();
    callback scene-clicked(float, float);
    callback tool-selected(string);
    callback console-cleared();
    callback asset-selected(string);
    callback property-changed(string, string);
    
    // Panel management callbacks
    callback panel-closed(string);
    callback panel-docked(string);
    callback panel-undocked(string);
    callback panel-moved(string, float, float);
    callback window-panel-requested(string);
    
    VerticalBox {
        // Menu bar
        MenuBar {
            file-new => { /* TODO */ }
            file-open => { /* TODO */ }
            file-save => { /* TODO */ }
            edit-undo => { /* TODO */ }
            edit-redo => { /* TODO */ }
            gameobject-create-empty => { object-created(); }
            gameobject-create-cube => { object-created(); }
            window-console => { window-panel-requested("console"); }
            window-hierarchy => { window-panel-requested("hierarchy"); }
            window-inspector => { window-panel-requested("inspector"); }
            window-project => { window-panel-requested("project"); }
            help-about => { /* TODO */ }
        }
        
        // Tool bar
        ToolBar {
            tool-select => { tool-selected("select"); }
            tool-move => { tool-selected("move"); }
            tool-rotate => { tool-selected("rotate"); }
            tool-scale => { tool-selected("scale"); }
        }
        
        // Main content area
        HorizontalBox {
            // Left panel - Hierarchy (docked)
            if !hierarchy-floating: DockablePanel {
                width: 250px;
                panel-id: "hierarchy";
                title: "Hierarchy";
                is-floating: false;
                
                panel-closed(id) => { panel-closed(id); }
                panel-docked(id) => { panel-docked(id); }
                panel-undocked(id) => { panel-undocked(id); }
                panel-moved(id, x, y) => { panel-moved(id, x, y); }
                
                HierarchyPanelContent {
                    objects: scene-objects;
                    selected-object: selected-object-index;
                    object-selected(index) => { object-selected(index); }
                    object-create => { object-created(); }
                    object-delete => { object-deleted(); }
                }
            }
            
            // Center area
            VerticalBox {
                // Scene view
                SceneView {
                    scene-clicked(x, y) => { scene-clicked(x, y); }
                }
                
                // Bottom panels
                HorizontalBox {
                    height: 200px;
                    
                    // Console (docked)
                    if !console-floating: DockablePanel {
                        panel-id: "console";
                        title: "Console";
                        is-floating: false;
                        
                        panel-closed(id) => { panel-closed(id); }
                        panel-docked(id) => { panel-docked(id); }
                        panel-undocked(id) => { panel-undocked(id); }
                        panel-moved(id, x, y) => { panel-moved(id, x, y); }
                        
                        ConsolePanelContent {
                            messages: console-messages;
                            clear-console => { console-cleared(); }
                        }
                    }
                    
                    // Project browser (docked)
                    if !project-floating: DockablePanel {
                        width: 300px;
                        panel-id: "project";
                        title: "Project";
                        is-floating: false;
                        
                        panel-closed(id) => { panel-closed(id); }
                        panel-docked(id) => { panel-docked(id); }
                        panel-undocked(id) => { panel-undocked(id); }
                        panel-moved(id, x, y) => { panel-moved(id, x, y); }
                        
                        ProjectPanelContent {
                            asset-selected(name) => { asset-selected(name); }
                            asset-imported => { /* TODO */ }
                        }
                    }
                }
            }
            
            // Right panel - Inspector (docked)
            if !inspector-floating: DockablePanel {
                width: 300px;
                panel-id: "inspector";
                title: "Inspector";
                is-floating: false;
                
                panel-closed(id) => { panel-closed(id); }
                panel-docked(id) => { panel-docked(id); }
                panel-undocked(id) => { panel-undocked(id); }
                panel-moved(id, x, y) => { panel-moved(id, x, y); }
                
                InspectorPanelContent {
                    selected-object: selected-object-index >= 0 && selected-object-index < scene-objects.length ? 
                        scene-objects[selected-object-index] : {
                            id: 0, name: "", position-x: 0, position-y: 0, position-z: 0,
                            rotation-x: 0, rotation-y: 0, rotation-z: 0,
                            scale-x: 1, scale-y: 1, scale-z: 1, active: true
                        };
                    has-selection: selected-object-index >= 0;
                    property-changed(prop, value) => { property-changed(prop, value); }
                }
            }
        }
    }
    
    // Floating panels layer
    if hierarchy-floating: DockablePanel {
        panel-id: "hierarchy";
        title: "Hierarchy";
        is-floating: true;
        panel-x: hierarchy-x;
        panel-y: hierarchy-y;
        panel-width: 250px;
        panel-height: 400px;
        
        panel-closed(id) => { panel-closed(id); }
        panel-docked(id) => { panel-docked(id); }
        panel-undocked(id) => { panel-undocked(id); }
        panel-moved(id, x, y) => { panel-moved(id, x, y); }
        
        HierarchyPanelContent {
            objects: scene-objects;
            selected-object: selected-object-index;
            object-selected(index) => { object-selected(index); }
            object-create => { object-created(); }
            object-delete => { object-deleted(); }
        }
    }
    
    if inspector-floating: DockablePanel {
        panel-id: "inspector";
        title: "Inspector";
        is-floating: true;
        panel-x: inspector-x;
        panel-y: inspector-y;
        panel-width: 300px;
        panel-height: 500px;
        
        panel-closed(id) => { panel-closed(id); }
        panel-docked(id) => { panel-docked(id); }
        panel-undocked(id) => { panel-undocked(id); }
        panel-moved(id, x, y) => { panel-moved(id, x, y); }
        
        InspectorPanelContent {
            selected-object: selected-object-index >= 0 && selected-object-index < scene-objects.length ? 
                scene-objects[selected-object-index] : {
                    id: 0, name: "", position-x: 0, position-y: 0, position-z: 0,
                    rotation-x: 0, rotation-y: 0, rotation-z: 0,
                    scale-x: 1, scale-y: 1, scale-z: 1, active: true
                };
            has-selection: selected-object-index >= 0;
            property-changed(prop, value) => { property-changed(prop, value); }
        }
    }
    
    if console-floating: DockablePanel {
        panel-id: "console";
        title: "Console";
        is-floating: true;
        panel-x: console-x;
        panel-y: console-y;
        panel-width: 600px;
        panel-height: 300px;
        
        panel-closed(id) => { panel-closed(id); }
        panel-docked(id) => { panel-docked(id); }
        panel-undocked(id) => { panel-undocked(id); }
        panel-moved(id, x, y) => { panel-moved(id, x, y); }
        
        ConsolePanelContent {
            messages: console-messages;
            clear-console => { console-cleared(); }
        }
    }
    
    if project-floating: DockablePanel {
        panel-id: "project";
        title: "Project";
        is-floating: true;
        panel-x: project-x;
        panel-y: project-y;
        panel-width: 400px;
        panel-height: 350px;
        
        panel-closed(id) => { panel-closed(id); }
        panel-docked(id) => { panel-docked(id); }
        panel-undocked(id) => { panel-undocked(id); }
        panel-moved(id, x, y) => { panel-moved(id, x, y); }
        
        ProjectPanelContent {
            asset-selected(name) => { asset-selected(name); }
            asset-imported => { /* TODO */ }
        }
    }
}