component TitleBar inherits Rectangle {
    height: 25px;
    background: #3c3c3c;
    
    in property <string> title: "Panel";
    in property <bool> is-floating: false;
    
    callback close-clicked();
    callback dock-clicked();
    callback undock-clicked();
    callback drag-started(float, float);
    callback dragging(float, float);
    callback drag-ended();
    
    property <bool> is-dragging: false;
    property <float> drag-start-x: 0;
    property <float> drag-start-y: 0;
    
    HorizontalBox {
        padding: 3px;
        
        // Drag handle
        Rectangle {
            width: 8px;
            background: is-dragging ? #666 : (drag-area.has-hover ? #555 : transparent);
            
            TouchArea {
                drag-area := TouchArea {
                    mouse-cursor: is-floating ? MouseCursor.move : MouseCursor.default;
                    
                    pointer-event(event) => {
                        if (event.kind == PointerEventKind.down && is-floating) {
                            is-dragging = true;
                            drag-start-x = event.position.x;
                            drag-start-y = event.position.y;
                            drag-started(event.position.x, event.position.y);
                        } else if (event.kind == PointerEventKind.up) {
                            if (is-dragging) {
                                is-dragging = false;
                                drag-ended();
                            }
                        }
                    }
                    
                    moved => {
                        if (is-dragging) {
                            dragging(self.mouse-x - drag-start-x, self.mouse-y - drag-start-y);
                        }
                    }
                }
            }
            
            // Drag handle visual
            VerticalBox {
                padding: 2px;
                spacing: 1px;
                Rectangle { height: 1px; background: #666; }
                Rectangle { height: 1px; background: #666; }
                Rectangle { height: 1px; background: #666; }
            }
        }
        
        Text {
            text: title;
            color: white;
            font-weight: 700;
            font-size: 11px;
            vertical-alignment: center;
        }
        
        Rectangle { /* spacer */ }
        
        // Dock/Undock button
        if is-floating: Button {
            text: "‚öì";
            max-width: 20px;
            max-height: 18px;
            clicked => { dock-clicked(); }
        }
        
        if !is-floating: Button {
            text: "üìå";
            max-width: 20px;
            max-height: 18px;
            clicked => { undock-clicked(); }
        }
        
        // Close button
        Button {
            text: "‚úï";
            max-width: 20px;
            max-height: 18px;
            clicked => { close-clicked(); }
        }
    }
}

component DockablePanel inherits Rectangle {
    in property <string> panel-id: "panel";
    in property <string> title: "Panel";
    in property <bool> is-floating: false;
    in property <float> panel-x: 0;
    in property <float> panel-y: 0;
    in property <float> panel-width: 300;
    in property <float> panel-height: 400;
    
    callback panel-closed(string);
    callback panel-docked(string);
    callback panel-undocked(string);
    callback panel-moved(string, float, float);
    
    // Panel styling
    background: #2d2d2d;
    border-width: is-floating ? 2px : 0px;
    border-color: is-floating ? #555 : transparent;
    drop-shadow-blur: is-floating ? 10px : 0px;
    drop-shadow-color: is-floating ? rgba(0, 0, 0, 0.5) : transparent;
    drop-shadow-offset-x: is-floating ? 3px : 0px;
    drop-shadow-offset-y: is-floating ? 3px : 0px;
    
    // Position (only for floating panels)
    x: is-floating ? panel-x : 0px;
    y: is-floating ? panel-y : 0px;
    width: is-floating ? panel-width : parent.width;
    height: is-floating ? panel-height : parent.height;
    
    VerticalBox {
        TitleBar {
            title: root.title;
            is-floating: root.is-floating;
            
            close-clicked => { panel-closed(panel-id); }
            dock-clicked => { panel-docked(panel-id); }
            undock-clicked => { panel-undocked(panel-id); }
            dragging(dx, dy) => { 
                panel-moved(panel-id, panel-x + dx, panel-y + dy);
            }
        }
        
        @children
    }
}

